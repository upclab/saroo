{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "Expo": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "react/prop-types": [0],
    "react/jsx-filename-extension": [0],
    "import/no-extraneous-dependencies": [0],
  }
}
