{
  "presets": ["babel-preset-expo"],
  "plugins": [
    "transform-decorators-legacy",
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "@": "./js",
        "@assets":  "./assets",
        "@screens": "./js/screens",
        "@components": "./js/components",
        "@mobx":  "./js/mobx",
        "@styles":  "./js/styles"
      }
    }]
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  }
}
